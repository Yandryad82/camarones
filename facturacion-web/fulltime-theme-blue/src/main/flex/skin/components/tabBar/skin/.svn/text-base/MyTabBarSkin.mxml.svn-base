<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:components="skin.components.tabBar.*" xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark" alpha.disabled="0.5">

	<fx:Metadata>
		[HostComponent("skin.components.tabBar.MyTabBar")]
	</fx:Metadata>


	<s:states>
		<s:State name="normal" />
		<s:State name="disabled" />
	</s:states>

	<s:DataGroup id="dataGroup" width="100%" height="100%">
		<s:layout>
			<s:ButtonBarHorizontalLayout gap="-12" />
		</s:layout>
		<s:itemRenderer>
			<fx:Component>
				<components:MyTabBarButton enabled="{data.enabled}" />
			</fx:Component>
		</s:itemRenderer>
	</s:DataGroup>
	

</s:Skin>
